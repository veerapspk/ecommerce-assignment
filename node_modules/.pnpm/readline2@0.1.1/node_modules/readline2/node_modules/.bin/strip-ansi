#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/nodejs/coding-practices/new-project/node_modules/.pnpm/strip-ansi@2.0.1/node_modules/strip-ansi/node_modules:/home/workspace/nodejs/coding-practices/new-project/node_modules/.pnpm/strip-ansi@2.0.1/node_modules:/home/workspace/nodejs/coding-practices/new-project/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-practices/new-project/node_modules:/home/workspace/nodejs/coding-practices/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/coding-practices/new-project/node_modules/.pnpm/readline2@0.1.1/node_modules/readline2/node_modules:/home/workspace/nodejs/coding-practices/new-project/node_modules/.pnpm/readline2@0.1.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/nodejs/coding-practices/new-project/node_modules/.pnpm/strip-ansi@2.0.1/node_modules/strip-ansi/node_modules:/home/workspace/nodejs/coding-practices/new-project/node_modules/.pnpm/strip-ansi@2.0.1/node_modules:/home/workspace/nodejs/coding-practices/new-project/node_modules/.pnpm/node_modules:/home/workspace/nodejs/coding-practices/new-project/node_modules:/home/workspace/nodejs/coding-practices/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/coding-practices/new-project/node_modules/.pnpm/readline2@0.1.1/node_modules/readline2/node_modules:/home/workspace/nodejs/coding-practices/new-project/node_modules/.pnpm/readline2@0.1.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../strip-ansi@2.0.1/node_modules/strip-ansi/cli.js" "$@"
else
  exec node  "$basedir/../../../../../strip-ansi@2.0.1/node_modules/strip-ansi/cli.js" "$@"
fi
