#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/workspace/nodejs/projects/new-project/ecommerce-assignment/node_modules/.pnpm/window-size@0.1.4/node_modules/window-size/node_modules:/home/workspace/nodejs/projects/new-project/ecommerce-assignment/node_modules/.pnpm/window-size@0.1.4/node_modules:/home/workspace/nodejs/projects/new-project/ecommerce-assignment/node_modules/.pnpm/node_modules:/home/workspace/nodejs/projects/new-project/ecommerce-assignment/node_modules:/home/workspace/nodejs/projects/new-project/node_modules:/home/workspace/nodejs/projects/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/projects/new-project/ecommerce-assignment/node_modules/window-size/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/workspace/nodejs/projects/new-project/ecommerce-assignment/node_modules/.pnpm/window-size@0.1.4/node_modules/window-size/node_modules:/home/workspace/nodejs/projects/new-project/ecommerce-assignment/node_modules/.pnpm/window-size@0.1.4/node_modules:/home/workspace/nodejs/projects/new-project/ecommerce-assignment/node_modules/.pnpm/node_modules:/home/workspace/nodejs/projects/new-project/ecommerce-assignment/node_modules:/home/workspace/nodejs/projects/new-project/node_modules:/home/workspace/nodejs/projects/node_modules:/home/workspace/nodejs/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/nodejs/projects/new-project/ecommerce-assignment/node_modules/window-size/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../window-size/cli.js" "$@"
else
  exec node  "$basedir/../window-size/cli.js" "$@"
fi
